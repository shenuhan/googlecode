package fr.game.battlefield;

import fr.game.fight.BattleFighter;

public class Tile {
	private int i;
	private int j;
	private int k;
	
	private BattleFighter fighter;
	
	public Tile(int i, int j, int k) {
		this.i = i;
		this.j = j;
		this.k = k;
	}

	public BattleFighter getFighter() {
		return fighter;
	}
	public void setFighter(BattleFighter fighter) {
		this.fighter = fighter;
	}
	public int getI() {
		return i;
	}
	public int getJ() {
		return j;
	}
	public int getK() {
		return k;
	}
}
